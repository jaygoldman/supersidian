{
  "_comment": "Copy this file to supersidian.config.json and customize for your setup",
  "bridges": [
    {
      "name": "personal",
      "enabled": true,
      "supernote_subdir": "Personal",
      "vault_path": "/Users/yourname/Documents/Obsidian/PersonalVault",
      "export_images": true,
      "images_subdir": "Supersidian/Assets",
      "extra_tags": ["supernote", "personal"],
      "aggressive_cleanup": false,
      "spellcheck": false
    },
    {
      "name": "work",
      "enabled": true,
      "supernote_subdir": "Work",
      "vault_path": "/Users/yourname/Documents/Obsidian/WorkVault",
      "export_images": true,
      "images_subdir": "Assets",
      "extra_tags": ["supernote", "work"],
      "aggressive_cleanup": true,
      "spellcheck": false
    },
    {
      "name": "archive",
      "enabled": false,
      "supernote_subdir": "Archive",
      "vault_path": "/Users/yourname/Documents/Obsidian/Archive",
      "extra_tags": ["archived"],
      "aggressive_cleanup": false,
      "spellcheck": false
    }
  ]
}
